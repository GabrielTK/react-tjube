{
  "name": "react.tjube.ninja",
  "version": "1.0.0",
  "description": "tjube.ninja rewrite using react",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:tjallingt/react-tjube.git"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --ext .js --ext .jsx app.js src",
    "watch": "concurrently -k -t HH:mm -p \"[{index} - {time}]\" \"npm run watch:screen\" \"npm run watch:remote\"",
    "build": "npm run build:production:screen && npm run build:production:remote",
    "build:screen": "cross-env NODE_ENV=production browserify src/screen.jsx -t babelify --extension=jsx | uglifyjs > static/js/screen.js",
    "build:remote": "cross-env NODE_ENV=production browserify src/remote.jsx -t babelify --extension=jsx | uglifyjs > static/js/remote.js",
    "watch:screen": "watchify src/screen.jsx -o static/js/screen.js -t babelify --extension=jsx -d -v",
    "watch:remote": "watchify src/remote.jsx -o static/js/remote.js -t babelify --extension=jsx -d -v"
  },
  "author": "tjallingt",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^6.0.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "concurrently": "^2.1.0",
    "cross-env": "^1.0.7",
    "eslint": "^2.0.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^1.0.4",
    "eslint-plugin-react": "^5.0.0",
    "uglify-js": "^2.6.2",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "body-parser": "^1.14.1",
    "classnames": "^2.2.0",
    "express": "^4.13.3",
    "mustache-express": "^1.2.2",
    "react": "^15.0.0",
    "react-addons-css-transition-group": "^15.0.0",
    "react-addons-update": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-youtube": "^6.0.0",
    "socket.io": "^1.3.7",
    "whatwg-fetch": "^1.0.0"
  }
}
